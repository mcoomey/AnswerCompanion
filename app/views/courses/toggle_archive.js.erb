$('#edit_course_<%= @course.id %>').parent().parent().remove();
<% if @course.archived? %>
  $("#archived-tab table").append("<%= j render 'course_table_row', :local_course => @course %>");
<% else %>
  $("#current-tab table").append("<%= j render 'course_table_row', :local_course => @course %>");
<% end %>
