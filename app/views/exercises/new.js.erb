<% if @new_section_title  %>
  $("#create_new_section_title_dialog").html("<%= j render 'new_section_title_dialog' %>");
  
  <% if @section_title.name? %>
  	$("#create_new_section_title_dialog").prepend("<span class=\"title\"> <%= j @section_title.name %> </span>");
  	$("#create_new_section_title_dialog").dialog({
  	  title: "Create New Section Title?",
  	  resizeable: false,
  	  modal: true
  	  });
  <% else %>
  	$("#create_new_section_title_dialog").dialog({
  	  title: "Leave Section Title blank ?",
  	  resizeable: false,
  	  modal: true
  	  });
  <% end %>

	$("#confirm_yes").click(function(){
		$("#create_new_section_title_dialog").dialog("close");
	});

	$("#confirm_no").click(function(){
		$("#subject_category_name").val("");
		$("#create_new_section_title_dialog").dialog("close");
	});

<% else %>
  ujsMessages("<%= @ujsAlert %>", "<%= @ujsNotice %>");
  $("#ujsform").remove();
	$("#buttonarea").hide().after('<%= j render "new_exercise" %>');
	$("#exercise_section_title_name").autocomplete({
		  minLength: 0,
		  delay: 0,
		  source: $("#exercise_section_title_name").data("autocomplete-source")
			});
<% end %>
		