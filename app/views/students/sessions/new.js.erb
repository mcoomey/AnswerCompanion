$("#menu").hide();
$(".notice").empty();
$(".alert").empty();
$("#videoblock").hide();
$("#login-register").html("<%= j render 'shared/login' %>").show();
$(".cancel-login").click(function() {
  $("#login-register").hide();
  $(".notice").empty();
  $(".alert").empty();
  $("#videoblock").show();
  $("#menu").show();
})