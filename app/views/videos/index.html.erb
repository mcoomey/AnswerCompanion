<article>
	<div id="buttonarea">
    <%= render 'shared/common_buttons' %>
    <%= render 'local_buttons' %>
	</div>
  
	<div id="horizontal-tabs">
  	<ul class="static-tabs-nav">
  		<li><a href=".current-tab">Current</a></li>
  		<li><a href=".archived-tab">Archived</a></li>
  		<li><a href=".future-tab">Future</a></li>
  	</ul>
	</div>
  
  <%= render 'shared/vertical_tabs' %>
  
	<div id="noscroll-panel">
    <div id="videotables">
  		<div class="current-tab">
        <div id="current-videotable"  class="tableContainer">
  				<%= render "video_table", :local_videos => @videos_current, local_course_asset_id: @course_asset.id %>
        </div>
  			<br class="clearfloat"/>				
  		</div>
  
  		<div class="archived-tab">
        <div id="archived-videotable"  class="tableContainer">
  				<%= render "video_table", :local_videos => @videos_archived, local_course_asset_id: @course_asset.id %>
        </div>
  			<br class="clearfloat"/>
  		</div>
  
  		<div class="future-tab">
        <div id="future-videotable"  class="tableContainer">
  				<%= render "video_table", :local_videos => @videos_future, local_course_asset_id: @course_asset.id %>
        </div>
  			<br class="clearfloat"/>
  		</div>     
      <br class="clearfloat"/>
    </div>
	</div>
  
</article>
