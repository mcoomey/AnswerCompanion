<div id="videopane"  class="tableContainer">
  <table>
    <thead class="fixedHeader">
  	  <tr>
  	    <th>Length</th>
  	    <th>Instructor</th>
  			<th>Play/Purchase</th>
  			<th class="replace"></th>
  			<th class="destroy"></th>
  	  </tr>
    </thead>
    <tbody class="scrollContent sortableTable">
  		<% local_videos.each do |video| %>
  			<%= render 'video_table_row', :local_video => video %>
  		<% end %>
  	</tbody>
  </table>
</div>